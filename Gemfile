# Gemfile
source "https://rubygems.org"

# Указываем версию Jekyll
gem "jekyll", "~> 4.4"

# Указываем плагин jekyll-feed, который используется в _config.yml
gem "jekyll-feed", "~> 0.12"

# Если вы используете другие плагины, указанные в _config.yml в секции plugins,
# их также нужно добавить сюда. В вашем случае - только jekyll-feed.